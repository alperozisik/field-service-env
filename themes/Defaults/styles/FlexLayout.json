{
	".flexLayout": {
		"height": 200,
		"width": 200,
		"touchEnabled": true,
		"visibility": true,
		"style": {
			"backgroundColor": "rgba(0,0,0,0)",
			"opacity": 1,
			"borderColor": "rgba(0,0,0,1)",
			"borderWidth": 0,
			"font": {}
		},
		"flexProps": {
			"alignSelf": "AUTO",
			"direction": "INHERIT",
			"alignContent": "STRETCH",
			"alignItems": "STRETCH",
			"justifyContent": "FLEX_START",
			"flexWrap": "NOWRAP",
			"flexDirection": "COLUMN",
			"positionType": "RELATIVE"
		},
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		},
		"&-warning": {
			"style": {
				"backgroundColor": "#9F6000"
			}
		},
		"&-error": {
			"style": {
				"backgroundColor": "#D8000C"
			}
		},
		"&-success": {
			"style": {
				"backgroundColor": "#4F8A10"
			}
		},
		"&-layoutSide": {
			"style": {
				"width": 0,
				"height": 0,
				"top": 0,
				"bottom": 0,
				"flexProps": {
					"flexGrow": 1
				}
			}
		},
		"&-layoutMiddle": {
			"style": {
				"width": 0,
				"height": 0,
				"top": 0,
				"bottom": 0,
				"flexProps": {
					"flexGrow": 5
				}
			}
		}
	}
}
