{
  ".label": {
    "width": 250,
    "height": 70,
    "touchEnabled": true,
    "text": "Label",
    "visibility": true,
    "multiline": true,
    "style": {
      "backgroundColor": "rgba(0,0,0,0)",
      "opacity": 1,
      "borderColor": "rgba(0,0,0,1)",
      "borderWidth": 0,
      "color": "#000000",
      "textAlignment": "MIDLEFT",
      "font": {
        "size": 16,
        "bold": false,
        "color": "#000000",
        "italic": false
      }
    },
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    },
    "&-warning": {
      "style": {
        "backgroundColor": "#9F6000"
      }
    },
    "&-error": {
      "style": {
        "backgroundColor": "#D8000C"
      }
    },
    "&-success": {
      "style": {
        "backgroundColor": "#4F8A10"
      }
    },
    "&-title": {
      "style": {
        "color": "#3C4350",
        "font": {
          "size": 22.5,
          "bold": false,
          "color": "#3C4350",
          "italic": false
        }
      }
    },
    "&-subTitle": {
      "style": {
        "color": "#696D6F",
        "font": {
          "size": 15,
          "bold": false,
          "color": "#696D6F",
          "italic": false
        }
      }
    }
  }
}
