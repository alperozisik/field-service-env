{
  ".mapView": {
    "height": 200,
    "width": 200,
    "touchEnabled": true,
    "rotateEnabled": true,
    "compassEnabled": true,
    "scrollEnabled": true,
    "type": "NORMAL",
    "visibility": true,
    "style": {
      "backgroundColor": "#FFFFFF",
      "opacity": 1,
      "borderColor": "rgba(0,0,0,1)",
      "borderWidth": 0,
      "font": {}
    },
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    },
    "&-warning": {
      "style": {
        "backgroundColor": "#9F6000"
      }
    },
    "&-error": {
      "style": {
        "backgroundColor": "#D8000C"
      }
    },
    "&-success": {
      "style": {
        "backgroundColor": "#4F8A10"
      }
    }
  }
}
